{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":preserveSemverRanges",
    ":disableRateLimiting"
  ],
  "assigneesFromCodeOwners": true,
  "labels": ["dependencies"],
  "packageRules": [
    {
      "groupName": "commitlint",
      "matchPackageNames": ["/commitlint/"]
    }
  ]
}
