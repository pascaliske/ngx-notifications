{
  "name": "ngx-notifications",
  "description": "Notification module for Angular v6+.",
  "version": "1.0.1",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-notifications.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-notifications/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-notifications-demo",
    "build": "ng build --prod",
    "postbuild": "cp README.md dist/ngx-notifications/README.md && cp LICENSE.md dist/ngx-notifications/LICENSE.md",
    "test": "ng test ngx-notifications",
    "lint": "ng lint ngx-notifications",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "lint-staged": {
    "{src,projects}/**/*.ts": [
      "yarn run lint",
      "yarn run format",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.2",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.3",
    "@angular-devkit/build-ng-packagr": "~0.7.3",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.8",
    "@types/uuid": "^3.4.3",
    "codelyzer": "~4.4.3",
    "husky": "^0.14.3",
    "jasmine-core": "~3.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "lint-staged": "^7.2.2",
    "ng-packagr": "^4.1.0",
    "prettier": "^1.14.2",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.7.2",
    "ts-node": "~7.0.1",
    "tsickle": ">=0.32.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~2.9.2"
  }
}
