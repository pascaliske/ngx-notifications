{
  "name": "ngx-notifications",
  "description": "Notification module for Angular v6+.",
  "version": "1.0.10",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-notifications.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-notifications/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-notifications-demo",
    "build": "ng build",
    "postbuild": "cp README.md dist/ngx-notifications/README.md && cp LICENSE.md dist/ngx-notifications/LICENSE.md",
    "test": "ng test ngx-notifications",
    "lint": "ng lint ngx-notifications",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "precommit": "lint-staged",
    "link": "yarn run build && cd dist/ngx-notifications && yarn link",
    "unlink": "cd dist/ngx-notifications && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "lint-staged": {
    "{src,projects}/**/*.ts": [
      "yarn run lint",
      "yarn run format",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/router": "^7.0.1",
    "@pascaliske/html-helpers": "^1.2.2",
    "core-js": "^2.5.7",
    "marked": "^0.5.1",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.3",
    "@angular-devkit/build-ng-packagr": "~0.10.3",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/marked": "^0.4.2",
    "@types/node": "10.12.8",
    "@types/uuid": "^3.4.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.1.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "lint-staged": "^8.0.0",
    "ng-packagr": "^4.4.0",
    "prettier": "^1.14.3",
    "protractor": "~5.4.1",
    "rxjs-tslint-rules": "^4.10.0",
    "ts-node": "~7.0.1",
    "tsickle": "^0.33.1",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~3.1.3"
  }
}
