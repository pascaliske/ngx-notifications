{
  "name": "ngx-notifications",
  "description": "Notification module for Angular v6+.",
  "version": "1.3.2",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-notifications.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-notifications/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-notifications-demo",
    "build": "run-s build:lib copy:readme copy:license",
    "build:lib": "ng build --prod",
    "copy:readme": "cp README.md dist/ngx-notifications/README.md",
    "copy:license": "cp LICENSE.md dist/ngx-notifications/LICENSE.md",
    "lint": "ng lint",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && cd dist/ngx-notifications && yarn link",
    "unlink": "cd dist/ngx-notifications && yarn unlink"
  },
  "prettier": "@pascaliske/prettier-config",
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.7",
    "@angular/common": "^9.0.0-rc.7",
    "@angular/compiler": "^9.0.0-rc.7",
    "@angular/core": "^9.0.0-rc.7",
    "@angular/forms": "^9.0.0-rc.7",
    "@angular/platform-browser": "^9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.7",
    "@angular/router": "^9.0.0-rc.7",
    "@pascaliske/html-helpers": "^1.5.7",
    "core-js": "^3.6.1",
    "rxjs": "^6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.7",
    "@angular/cli": "~9.0.0-rc.7",
    "@angular/compiler-cli": "^9.0.0-rc.7",
    "@angular/language-service": "^9.0.0-rc.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@pascaliske/prettier-config": "^1.0.2",
    "@types/marked": "^0.7.2",
    "@types/node": "^13.1.1",
    "@types/uuid": "^3.4.6",
    "codelyzer": "^5.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "marked": "^0.8.0",
    "ng-packagr": "^9.0.0-rc.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rxjs-tslint-rules": "^4.28.0",
    "standard-changelog": "^2.0.21",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.6.4"
  }
}
