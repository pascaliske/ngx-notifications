{
  "name": "ngx-notifications",
  "description": "Notification module for Angular v6+.",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-notifications.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-notifications/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-notifications-demo",
    "build": "ng build --prod",
    "postbuild": "cp README.md dist/ngx-notifications/README.md && cp LICENSE.md dist/ngx-notifications/LICENSE.md",
    "lint": "ng lint",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "link": "yarn run build && cd dist/ngx-notifications && yarn link",
    "unlink": "cd dist/ngx-notifications && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@pascaliske/html-helpers": "^1.2.2",
    "core-js": "^2.6.1",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@types/marked": "^0.6.0",
    "@types/node": "10.12.21",
    "@types/uuid": "^3.4.4",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "marked": "^0.6.0",
    "ng-packagr": "^4.7.0",
    "prettier": "^1.15.3",
    "rxjs-tslint-rules": "^4.14.3",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.1.6"
  }
}
