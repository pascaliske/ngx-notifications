{
  "name": "ngx-notifications",
  "description": "Notification module for Angular v6+.",
  "version": "1.3.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-notifications.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-notifications/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-notifications-demo",
    "build": "ng build --prod",
    "postbuild": "cp README.md dist/ngx-notifications/README.md && cp LICENSE.md dist/ngx-notifications/LICENSE.md",
    "lint": "ng lint",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && cd dist/ngx-notifications && yarn link",
    "unlink": "cd dist/ngx-notifications && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@pascaliske/html-helpers": "^1.2.2",
    "core-js": "^3.0.0",
    "rxjs": "^6.4.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/marked": "^0.6.0",
    "@types/node": "12.0.8",
    "@types/uuid": "^3.4.4",
    "codelyzer": "^5.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.1.1",
    "marked": "^0.6.1",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.16.3",
    "rxjs-tslint-rules": "^4.23.3",
    "standard-changelog": "^2.0.6",
    "ts-node": "~8.2.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "~5.17.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.4.5"
  }
}
